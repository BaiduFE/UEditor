    width: 60px;
    font-size: 12px;
    height: 20px;
    line-height: 20px;
    padding-left: 5px;
    white-space: nowrap;
}
.edui-toolbar .edui-combox-body .edui-arrow {
    background: url(images/icons.png) -741px 0;
