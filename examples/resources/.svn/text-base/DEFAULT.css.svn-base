    width: 8px;
}
.edui-toolbar .edui-combox .edui-combox-body {
    border: 1px solid #CCC;
    background-color: white;
}
.edui-toolbar .edui-combox-body .edui-arrow {
    border-left: 1px solid #CCC;
}
.edui-toolbar .edui-state-hover .edui-combox-body {
