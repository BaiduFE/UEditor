<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN"
        "http://www.w3.org/TR/html4/loose.dtd">
<html>
<head>
    <title></title>
    <script src="jslitmus.js" type="text/javascript"></script>
    <script src="../../../src/editor.js" type="text/javascript"></script>
    <script  src="../../../src/core/utils.js" type="text/javascript"></script>
    <script  src="../../../src/core/browser.js" type="text/javascript"></script>
    <script  src="../../../src/core/dom/dom.js" type="text/javascript"></script>
    <script  src="../../../src/core/dom/dtd.js" type="text/javascript"></script>
    <script  src="../../../src/core/dom/domUtils.js" type="text/javascript"></script>
</head>
<body>
<div id="a" style="font-size:14px; background-color: red;"></div>
<div id="b" style="font-size:14px; background-color: black;"></div>
<div id="c" style=""></div>
<div id="d" style="color:red;"></div>
<script type="text/javascript">
    var domUtils = baidu.editor.dom.domUtils;
    var a = document.getElementById('a');
    var b = document.getElementById('b');
    var c = document.getElementById('c');
    JSLitmus.test('a, b', function (k){
        while (k --) {
            domUtils.isSameStyle(a, b);
        }
    });
    JSLitmus.test('b, c', function (k){
        while (k --) {
            domUtils.isSameStyle(b, c);
        }
    });
    JSLitmus.test('b, d', function (k){
        while (k --) {
            domUtils.isSameStyle(b, d);
        }
    });
</script>
</body>
</html>